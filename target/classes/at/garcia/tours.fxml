<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.TextFlow?>

<Pane xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.garcia.controller.ToursController"
      xmlns="http://javafx.com/javafx/11.0.1"
      maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
      prefWidth="800.0">
    <children>
        <VBox prefHeight="600.0" prefWidth="800.0">
            <children>
                <MenuBar>
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Close"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Edit">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Delete"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help">
                            <items>
                                <MenuItem mnemonicParsing="false" text="About"/>
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <TextFlow prefHeight="200.0" prefWidth="200.0">
                    <padding>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0"/>
                    </padding>
                </TextFlow>
                <BorderPane prefHeight="600.0" prefWidth="200.0">
                    <left>
                        <VBox prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
                    </left>
                    <right>
                        <ScrollBar orientation="VERTICAL" BorderPane.alignment="CENTER"/>
                    </right>
                </BorderPane>
                <AnchorPane prefHeight="100.0" prefWidth="200.0"/>
            </children>
        </VBox>
    </children>
</Pane>
